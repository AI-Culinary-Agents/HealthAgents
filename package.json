{
  "dependencies": {
    "tavily": "^1.0.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "install:server": "cd server && npm install",
    "install:website": "cd website && npm install",
    "install:all": "npm-run-all install:server install:website",
    "start:flask": "cd server && flask run",
    "start:website": "cd website && npm run dev",
    "start:all": "npm-run-all --parallel start:flask start:website"
  }
}
